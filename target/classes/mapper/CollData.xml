<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.linsy.prt.dao.CollDataDao">    <insert id="UpsertCollData"  parameterType="com.linsy.prt.domain.T_JHS_CollData"  useGeneratedKeys="false">        UPSERT INTO PRT.T_JHS_CollData(JuId,Starttime,Endtime,TreasureID,TreasureLink,TreasureName,SaleQty,StockQty,SumStockQty,ActPrice,Price,ShopName,Act_id,PicUrl,RemindNum,CreateDate,Updatetime,IsExpire)        VALUES(#{JuId},#{Starttime},#{Endtime},#{TreasureID},#{TreasureLink},#{TreasureName},#{SaleQty},#{StockQty},#{SumStockQty},#{ActPrice},#{Price},#{ShopName},#{Act_id},#{PicUrl},#{RemindNum},#{CreateDate},#{Updatetime},#{IsExpire})    </insert>    <select id="GetCollDataAll"  resultType="T_JHS_CollData">        select * from PRT.T_JHS_CollData  limit 100    </select></mapper>