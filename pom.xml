/includes>                        </resource>                    </webResources>                </configuration>            </plugin>            <!--deploy skip-->            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-deploy-plugin</artifactId>                <version>2.8.2</version>                <configuration>                    <skip>true</skip>                </configuration>            </plugin>        </plugins>    </build>    </project>