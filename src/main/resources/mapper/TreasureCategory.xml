<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.linsy.prt.dao.TreasureCategoryDao">    <insert id="UpsertTreasureCategory"  parameterType="com.linsy.prt.domain.T_JHS_TreasureCategory"  useGeneratedKeys="false">        UPSERT INTO PRT.T_JHS_TreasureCategory(TreasureID,ShopName,CategoryName,StyleName,Status,UpdateDate)        VALUES(#{TreasureID},#{ShopName},#{CategoryName},#{StyleName},#{Status},#{UpdateDate})    </insert>    <select id="GetTreasureCategory"  resultType="T_JHS_TreasureCategory" parameterType="String">        select TreasureID,ShopName,CategoryName,StyleName,Status,UpdateDate from PRT.T_JHS_TreasureCategory where TreasureID=#{TreasureID}    </select>    <select id="GetTreasureCategoryAll"  resultType="T_JHS_TreasureCategory">        select TreasureID,ShopName,CategoryName,StyleName,Status,UpdateDate from PRT.T_JHS_TreasureCategory  limit 100    </select></mapper>